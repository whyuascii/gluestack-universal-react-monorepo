{
  "name": "database",
  "version": "0.0.0",
  "description": "Shared database schemas and connection using Drizzle ORM and PostgreSQL",
  "keywords": [
    "database",
    "drizzle",
    "postgresql",
    "orm",
    "zod"
  ],
  "author": "whyuascii <eddy@whyuascii.com>",
  "license": "ISC",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src",
    "drizzle"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "db:seed": "tsx scripts/seed.ts",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "generate": "drizzle-kit generate:pg"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.7.0",
    "pg": "^8.16.3",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/pg": "^8.15.6",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-config": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
