# PostHog Analytics
EXPO_PUBLIC_POSTHOG_KEY=your_posthog_project_api_key
EXPO_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Or for self-hosted PostHog:
# EXPO_PUBLIC_POSTHOG_HOST=https://your-posthog-instance.com

# RevenueCat Configuration
# Get your API key from https://app.revenuecat.com/
# Use test_* keys for development, production keys for live apps
EXPO_PUBLIC_REVENUECAT_API_KEY=test_

# Entitlement identifier (optional, defaults to "Nest Quest")
# Must match your RevenueCat dashboard configuration
# EXPO_PUBLIC_REVENUECAT_ENTITLEMENT_PREMIUM=Nest Quest

# Product identifiers (optional, defaults shown)
# Must match App Store Connect / Google Play Console
# EXPO_PUBLIC_REVENUECAT_PRODUCT_MONTHLY=monthly
# EXPO_PUBLIC_REVENUECAT_PRODUCT_YEARLY=yearly

# Default offering identifier (optional, defaults to "default")
# EXPO_PUBLIC_REVENUECAT_DEFAULT_OFFERING=default
