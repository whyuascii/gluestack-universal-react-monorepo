{
  "name": "@app/mobile",
  "version": "1.0.0",
  "description": "Expo React Native mobile app with NativeWind and gluestack components",
  "keywords": [
    "mobile",
    "expo",
    "react-native",
    "nativewind",
    "gluestack"
  ],
  "author": "",
  "license": "ISC",
  "private": true,
  "type": "module",
  "main": "./index.js",
  "files": [
    "src",
    ".expo"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "expo start",
    "dev:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo export",
    "build:dev:ios": "eas build --platform ios --profile development",
    "build:dev:android": "eas build --platform android --profile development",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:prod:ios": "eas build --platform ios --profile production",
    "build:prod:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"No tests yet\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .expo",
    "doctor": "npx expo-doctor"
  },
  "dependencies": {
    "@novu/react-native": "^3.13.0",
    "@app/ads": "workspace:*",
    "@app/analytics": "workspace:*",
    "@app/auth": "workspace:*",
    "@app/components": "workspace:*",
    "@app/config": "workspace:*",
    "@app/i18n": "workspace:*",
    "@app/notifications": "workspace:*",
    "@app/subscriptions": "workspace:*",
    "@app/ui": "workspace:*",
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.62.11",
    "expo": "~54.0.12",
    "expo-application": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.8",
    "expo-router": "~6.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "lucide-react-native": "^0.545.0",
    "nativewind": "^4.2.1",
    "posthog-react-native": "^3.3.11",
    "posthog-react-native-session-replay": "^1.2.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^16.0.1",
    "react-native-purchases": "^9.6.10",
    "react-native-purchases-ui": "^9.6.10",
    "react-native-safe-area-context": "~5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "~15.12.0",
    "react-native-webview": "~13.15.0",
    "react-native-worklets": "^0.5.1"
  },
  "devDependencies": {
    "@app/eslint-config": "workspace:*",
    "@app/tailwind-config": "workspace:*",
    "@app/typescript-config": "workspace:*",
    "@babel/core": "^7.25.2",
    "@types/react": "^19.1.0",
    "baseline-browser-mapping": "^2.9.6",
    "eslint": "^9.18.0",
