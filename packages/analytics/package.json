{
  "name": "@app/analytics",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./web": {
      "types": "./dist/web.d.ts",
      "default": "./dist/web.js"
    },
    "./mobile": {
      "types": "./dist/mobile.d.ts",
      "default": "./dist/mobile.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:swc && pnpm run build:types",
    "build:swc": "swc src -d dist --strip-leading-paths --copy-files",
    "build:types": "tsc --emitDeclarationOnly --outDir dist --skipLibCheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@opentelemetry/api-logs": "^0.57.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.57.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-logs": "^0.57.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "posthog-js": "1.329.0",
    "posthog-node": "^4.0.0"
  },
  "optionalDependencies": {
    "posthog-react-native": "^4.24.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.6",
    "@types/react": "^19.0.1",
    "eslint": "^9.18.0",
    "@app/eslint-config": "workspace:*",
    "typescript": "^5.9.3",
    "@app/typescript-config": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-native": "*",
    "fastify": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "fastify": {
      "optional": true
    }
  }
}
